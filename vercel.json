{
  "version": 2,
  "projects": [
    {
      "name": "ai-robot-frontend",
      "root": "./ai-robot-frontend",
      "builds": [
        {
          "src": "package.json",
          "use": "@vercel/static-build",
          "config": {
            "installCommand": "npm install",
            "buildCommand": "npm run build",
            "outputDirectory": "dist"
          }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/$1",
          "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
          }
        }
      ]
    }
  ]
}